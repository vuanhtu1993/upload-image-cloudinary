{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "build": {
        "script": "npm run start"
    },
    "routes": [
        {
            "src": "/.*",
            "use": "/",
            "dest": "index.js"
        }
    ]
}